{
  "name": "qualia-3d",
  "version": "0.1.0",
  "description": "A high-performance 3D physics sandbox and game engine using Rapier WASM and Three.js",
  "author": "Qualia Team",
  "license": "MIT",
  "private": true,
  "type": "module",
  "keywords": [
    "3d",
    "physics",
    "game-engine",
    "threejs",
    "rapier",
    "angular",
    "typescript",
    "webgl",
    "wasm"
  ],
  "homepage": "https://github.com/AxisRotunda/Qualia#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AxisRotunda/Qualia.git"
  },
  "bugs": {
    "url": "https://github.com/AxisRotunda/Qualia/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "node scripts/dev-server.cjs dev",
    "dev:clean": "node scripts/dev-server.cjs dev:clean",
    "dev:fresh": "node scripts/dev-server.cjs dev:fresh",
    "build": "node scripts/dev-server.cjs build",
    "build:prod": "node scripts/dev-server.cjs build:prod",
    "preview": "node scripts/dev-server.cjs preview",
    "port:check": "node scripts/dev-server.cjs port:check",
    "port:free": "node scripts/dev-server.cjs port:free",
    "port:kill": "node scripts/dev-server.cjs port:free --force",
    "clean": "node scripts/dev-server.cjs clean",
    "clean:all": "node scripts/dev-server.cjs clean:all",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --config eslint.config.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,html}\"",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@dimforge/rapier3d-compat": "^0.19.3",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.0.0",
    "three": "^0.173.0",
    "tslib": "^2.8.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.14.0",
    "@types/three": "^0.173.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "globals": "^15.15.0",
    "prettier": "^3.4.0",
    "rimraf": "^6.0.0",
    "typescript": "~5.9.0",
    "vite": "^6.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
